{
  "name": "redux-hacker-news",
  "version": "0.1.0",
  "description": "Hacker news clone using redux, react, webpack, babel, express, react-router.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "webpack",
    "dev": "node --harmony ./build/webpack-dev-server",
    "dev:debug": "npm run dev -- --debug",
    "dev:quiet": "npm run dev -- --quiet",
    "test": "npm run test:unit && npm run test:server",
    "test:unit": "node ./node_modules/karma/bin/karma start",
    "test:server": "mocha --harmony server/scripts/test.js",
    "deploy": "npm run test:unit && npm run compile && npm run test:server",
    "server:start": "node --harmony ./server/scripts/start"
  },
